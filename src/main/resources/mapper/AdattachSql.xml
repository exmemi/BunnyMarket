<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="data.mapper.AdattachMapper">
	<insert id="insert">
		insert into adattach (uuid,uploadpath,filename,filetype,idx)
			values (#{uuid},#{uploadpath},#{filename},#{filetype},#{idx})
	</insert>
	<delete id="delete">
		delete from adattach where uuid=#{uuid}
	</delete>
	<select id="findByIdx">
		select * from adattach where idx=#{idx}
	</select>
</mapper>